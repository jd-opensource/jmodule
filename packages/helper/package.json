{
  "name": "@jmodule/helper",
  "version": "1.0.4",
  "description": "JModule使用助手",
  "author": "feirpri <feirpri@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist && node ./build/build.js",
    "lint": "eslint --ext .js src"
  },
  "exports": {
    "./host/": "./dist/host/",
    "./app/": "./dist/app/"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "devDependencies": {
    "@jmodule/client": "^4.3.0-0",
    "@types/requestidlecallback": "^0.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "eslint": "8",
    "eslint-webpack-plugin": "^3.1.1",
    "ts-loader": "9.3.1",
    "tslib": "^2.0.0",
    "typescript": "^4.7.4",
    "vue": "2",
    "vue-router": "^3.0.1",
    "vue-router4": "npm:vue-router@4",
    "vue3": "npm:vue@^3.2.36",
    "vuex": "^3.0.1",
    "webpack": "^5"
  },
  "peerDependencies": {
    "@jmodule/client": "^4.3.0-0"
  },
  "optionalDependencies": {
    "vue": "2",
    "vue-router": "^3.0.1",
    "vue-router4": "npm:vue-router@4",
    "vue3": "npm:vue@^3.2.36",
    "vuex": "^3.0.1"
  },
  "files": [
    "src",
    "dist",
    "types"
  ],
  "typesVersions": {
    "*": {
      "app/app.vue3": [
        "types/app/app.vue3"
      ],
      "app/app.vue2": [
        "types/app/app.vue2"
      ],
      "host/resource.html": [
        "types/host/resource.html"
      ],
      "host/type.app": [
        "types/host/type.app"
      ],
      "host/type.routes.vue2": [
        "types/host/type.routes.vue2"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jdtdevops/jmodule.git"
  },
  "keywords": [
    "jmodule",
    "micro"
  ],
  "bugs": {
    "url": "https://github.com/jdtdevops/jmodule/issues"
  },
  "homepage": "https://jmodule.jd.com",
  "sideEffects": false
}
